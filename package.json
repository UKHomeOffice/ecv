{
  "name": "change-of-address",
  "version": "1.0.0",
  "description": "A service for changing address",
  "main": "index.js",
  "engines": {
    "node": ">=18.12.1"
  },
  "scripts": {
    "start": "node server.js",
    "test:ui-integration": "NODE_ENV=test _mocha \"test/_ui-integration/**/*.spec.js\" --exit",
    "start:dev": "hof-build watch",
    "build": "hof-build",
    "postinstall": "hof-build",
    "test:lint": "eslint . --config ./node_modules/eslint-config-hof/default.js",
    "test:unit": "LOG_LEVEL=error nyc _mocha \"test/_unit/**/*.spec.js\"",
    "test:acceptance": "TAGS=\"${TAGS:=@feature}\" npm run test:cucumber",
    "test:acceptance_browser": "ACCEPTANCE_WITH_BROWSER=true TAGS=\"${TAGS:=@feature}\" yarn run test:cucumber",
    "test:cucumber": "cucumber-js -f @cucumber/pretty-formatter \"test/_features/**/*.feature\" --require test/_features/test.setup.js --require \"test/_features/step_definitions/**/*.js\" --tags $TAGS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/evc.git"
  },
  "author": "HOF",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/evc/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/evc#readme",
  "dependencies": {
    "accessible-autocomplete": "^2.0.4",
    "bl": "^6.0.12",
    "busboy": "^1.6.0",
    "crypto-random-string": "^5.0.0",
    "hof": "^20.5.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.2.1",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "chai": "^4.3.5",
    "eslint": "^8.57.0",
    "eslint-config-hof": "^1.3.4",
    "jquery": "^3.6.3",
    "jsdom": "^16.4.0",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "playwright": "^1.2.3",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "supertest": "^5.0.0",
    "supertest-session": "^4.1.0"
  },
  "mocha": {
    "reporter": "spec",
    "require": "test/setup.js",
    "recursive": "true",
    "timeout": "6000"
  }
}
